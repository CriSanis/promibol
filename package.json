            "dotenv": "^16.3.1",
            "express": "^4.18.2",
            "jsonwebtoken": "^9.0.1",
            "multer": "^1.4.5-lts.1",
            "pg": "^8.11.0",
            "postcss": "^8",
            "react": "^18.0.0",
            "react-dom": "^18.0.0",
            "react-redux": "^8.1.1",
            "react-router-dom": "^6.8.0",
            "react-scripts": "^5.0.0",
            "tailwind-merge": "^2.4.0",
